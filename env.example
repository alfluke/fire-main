# Database
DATABASE_URL="postgresql://username:password@localhost:5432/firestudio?schema=public"

# NextAuth
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-secret-key-here"

# Google AI (Genkit)
GOOGLE_AI_API_KEY="your-google-ai-api-key"

# Labelary API (opcional - para proxies)
LABELARY_BASE_URLS="https://api.labelary.com"

# Labelary - limites e robustez (recomendado)
LABELARY_MAX_CONCURRENCY="2"
LABELARY_MAX_ATTEMPTS="8"
LABELARY_FETCH_TIMEOUT_MS="25000"
LABELARY_CACHE_MAX_SIZE="256"

# PDF builder - paralelismo conservador para evitar 429
LABELARY_API_POOL_SIZE_MAX="3"
LABELARY_PDF_BATCH_SIZE="1"
LABELARY_BATCH_DELAY_MS="300"
# For√ßar uso do modo PNG em caso de necessidade (opcional: "png" ou vazio)
# LABELARY_PDF_BUILD_MODE=""

# Node Environment
NODE_ENV="development"

